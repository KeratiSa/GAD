repeat task.wait() until game:IsLoaded()

task.spawn(function()
    task.wait(10) -- à¸£à¸­à¹‚à¸«à¸¥à¸”à¸—à¸¸à¸à¸­à¸¢à¹ˆà¸²à¸‡

    local player = game:GetService("Players").LocalPlayer
    local leaderstats = player:WaitForChild("leaderstats", 30)
    local sheckles = leaderstats and leaderstats:WaitForChild("Sheckles", 30)
    if not sheckles then
        warn("à¹„à¸¡à¹ˆà¸žà¸š Sheckles")
        return
    end

    local loadedStart, loaded10m, loaded100m = false, false, false

    while true do
        local money = sheckles.Value
        print("ðŸ’° Sheckles:", money)

        if money >= 100_000_000 and not loaded100m then
            loaded100m = true
            print("ðŸ’¸ à¹‚à¸«à¸¥à¸”à¸ªà¸„à¸£à¸´à¸›à¸•à¹Œ 100m")
            local ok, res = pcall(function()
                return game:HttpGet("https://raw.githubusercontent.com/KeratiSa/GAD/refs/heads/main/100m")
            end)
            if ok then
                loadstring(res)()
            else
                warn("à¹‚à¸«à¸¥à¸” 100m à¸¥à¹‰à¸¡à¹€à¸«à¸¥à¸§:", res)
            end

        elseif money >= 10_000_000 then
            print("ðŸ’¸ à¹€à¸•à¸°à¹€à¸žà¸·à¹ˆà¸­à¹„à¸›à¸£à¸±à¸™ 10M à¸ à¸²à¸¢à¸™à¸­à¸")
            player:Kick("à¹€à¸‡à¸´à¸™à¸–à¸¶à¸‡ 10 à¸¥à¹‰à¸²à¸™ â€” à¸­à¸­à¸à¸ˆà¸²à¸à¹€à¸à¸¡à¹€à¸žà¸·à¹ˆà¸­à¹„à¸›à¸£à¸±à¸™à¸•à¸±à¸§ 10M")
            break

        elseif money >= 1_000_000 and not loaded10m then
            loaded10m = true
            print("ðŸ’¸ à¹‚à¸«à¸¥à¸”à¸ªà¸„à¸£à¸´à¸›à¸•à¹Œ 10m")
            local ok, res = pcall(function()
                return game:HttpGet("https://raw.githubusercontent.com/KeratiSa/GAD/refs/heads/main/10m")
            end)
            if ok then
                loadstring(res)()
            else
                warn("à¹‚à¸«à¸¥à¸” 10m à¸¥à¹‰à¸¡à¹€à¸«à¸¥à¸§:", res)
            end

        elseif money < 1_000_000 and not loadedStart then
            loadedStart = true
            print("ðŸ’° à¹‚à¸«à¸¥à¸”à¸ªà¸„à¸£à¸´à¸›à¸•à¹Œ Start")
            local ok, res = pcall(function()
                return game:HttpGet("https://raw.githubusercontent.com/KeratiSa/GAD/refs/heads/main/Start")
            end)
            if ok then
                loadstring(res)()
            else
                warn("à¹‚à¸«à¸¥à¸” Start à¸¥à¹‰à¸¡à¹€à¸«à¸¥à¸§:", res)
            end
        end

        task.wait(5)
    end
end)
