task.spawn(function()
    local loadedStart = false
    local loaded10m = false
    local loaded100m = false

    while true do
        local player = game:GetService("Players").LocalPlayer
        local sheckles = player.leaderstats:WaitForChild("Sheckles").Value

        print("Sheckles ‡∏õ‡∏±‡∏à‡∏à‡∏∏‡∏ö‡∏±‡∏ô:", sheckles)

        if sheckles >= 100_000_000 then
            if not loaded100m then
                loaded100m = true
                print("üí∏ ‡πÄ‡∏á‡∏¥‡∏ô‡∏ñ‡∏∂‡∏á 100 ‡∏•‡πâ‡∏≤‡∏ô ‚Üí ‡πÇ‡∏´‡∏•‡∏î‡∏™‡∏Ñ‡∏£‡∏¥‡∏õ‡∏ï‡πå 100m")
                pcall(function()
                    loadstring(game:HttpGet("https://raw.githubusercontent.com/KeratiSa/GAD/refs/heads/main/100m"))()
                end)
            end

        elseif sheckles >= 10_000_000 then
            print("üí∏ ‡πÄ‡∏á‡∏¥‡∏ô‡∏ñ‡∏∂‡∏á 10 ‡∏•‡πâ‡∏≤‡∏ô ‚Äî ‡πÄ‡∏ï‡∏∞‡∏≠‡∏≠‡∏Å‡πÄ‡∏Å‡∏°‡πÄ‡∏û‡∏∑‡πà‡∏≠‡πÑ‡∏õ‡∏£‡∏±‡∏ô‡∏ï‡∏±‡∏ß 10M")
            player:Kick("‡πÄ‡∏á‡∏¥‡∏ô‡∏ñ‡∏∂‡∏á 10 ‡∏•‡πâ‡∏≤‡∏ô ‚Äî ‡∏≠‡∏≠‡∏Å‡∏à‡∏≤‡∏Å‡πÄ‡∏Å‡∏°‡πÄ‡∏û‡∏∑‡πà‡∏≠‡πÑ‡∏õ‡∏£‡∏±‡∏ô‡∏ï‡∏±‡∏ß 10M")
            break

        elseif sheckles >= 1_000_000 then
            if not loaded10m then
                loaded10m = true
                print("üí∏ ‡πÄ‡∏á‡∏¥‡∏ô‡∏£‡∏∞‡∏´‡∏ß‡πà‡∏≤‡∏á 1 - 10 ‡∏•‡πâ‡∏≤‡∏ô ‚Üí ‡πÇ‡∏´‡∏•‡∏î‡∏™‡∏Ñ‡∏£‡∏¥‡∏õ‡∏ï‡πå 10m")
                pcall(function()
                    local src = game:HttpGet("https://raw.githubusercontent.com/KeratiSa/GAD/refs/heads/main/10m")
                    print("‡πÇ‡∏´‡∏•‡∏î‡∏™‡∏≥‡πÄ‡∏£‡πá‡∏à (10m):", #src, "‡∏ï‡∏±‡∏ß‡∏≠‡∏±‡∏Å‡∏©‡∏£")
                    loadstring(src)()
                end)
            end

        else
            if not loadedStart then
                loadedStart = true
                print("üí∞ ‡πÄ‡∏á‡∏¥‡∏ô‡∏ô‡πâ‡∏≠‡∏¢‡∏Å‡∏ß‡πà‡∏≤ 1 ‡∏•‡πâ‡∏≤‡∏ô ‚Üí ‡πÇ‡∏´‡∏•‡∏î‡∏™‡∏Ñ‡∏£‡∏¥‡∏õ‡∏ï‡πå Start")
                pcall(function()
                    loadstring(game:HttpGet("https://raw.githubusercontent.com/KeratiSa/GAD/refs/heads/main/Start"))()
                end)
            end
        end

        task.wait(5)
    end
end)
